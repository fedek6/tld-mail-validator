{
  "name": "fedek6/tld-mail-validator",
  "type": "library",
  "description": "PHP e-mail address validator using list of all known TLDs",
  "require": {
    "php": ">=7.4.0"
  },
  "authors": [
    {
      "name": "Konrad Fedorczyk",
      "email": "contact@realhe.ro"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Fedek6\\TldMailValidator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fedek6\\TldMailValidator\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox --verbose --colors tests"
  }
}
